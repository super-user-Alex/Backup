import fitz

doc = fitz.open("archivo.pdf")
page = doc[0]

# Inspeccionar esos Form XObjects espec√≠ficos
for xref in [122, 123]:
    print(f"\n=== XObject xref={xref} ===")
    print(doc.xref_object(xref))  # Ver el diccionario completo
    
    stream = doc.xref_stream(xref)
    if stream:
        print("\n--- Stream (primeros 500 bytes) ---")
        print(stream[:500])
    
    # Ver si tiene Patterns o Shadings en sus recursos
    for key in ["Resources/Pattern", "Resources/Shading", "Resources/XObject"]:
        val = doc.xref_get_key(xref, key)
        if val != ("null", "null"):
            print(f"\n{key}: {val}")
